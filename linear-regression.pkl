   },
   "outputs": [],
   "source": [
    "import statsmodels.api as sm\n",
    "ols=sm.OLS(Y_train,X_train).fit()\n",
    "#ols=sm.add_constant(1)\n",
    "print(ols.summary())\n"
   ]
